{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "plugins": [{ "name": "next" }],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowJs": false,
    "jsx": "react-jsx",
    "noEmit": true,
    "skipLibCheck": true,
    "incremental": true,
    "composite": false,
    "isolatedModules": true,
    "verbatimModuleSyntax": true
  }
}
